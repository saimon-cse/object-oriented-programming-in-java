package practiceExam;
import java.util.*;

class Main {
	
	
	void validateAge(int age) {
		if(age < 18) 
			throw new MyException("You are not eligible!");
		else 
			System.out.println("You are eligible.");
	}
	public static void main(String[] args) {
		
		int a = 10, b = 0;
		
		try {
			System.out.println(a/b);
		}
		catch(Exception e) {
			System.out.println(e);
		}
		finally {
			int c = b/a;
		}
		
		
	
		
	}    
}
